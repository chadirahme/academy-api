		<parameter name="studentid" class="java.lang.Long">
			<defaultValueExpression><![CDATA[$P{studentid}]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[select g.subjectname, s.studentid, s.studentname,s.rollnumber,  s.dayabsent,
m.academicyear,m.semester,s.grade,s.section,m.subject,m.finalmark,
m.subjectgrade
from marks m
inner join student s on m.studentid=s.studentid
inner join gradesubject g on g.subjectname=m.subject
where s.grade='Grade8' and s.section='A' and g.subjectlevel=1
and s.studentid=$P{studentid}
order by s.studentname , g.subjectorder]]>
		</queryString>
